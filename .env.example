# n8n Integration
# The webhook URL from your n8n workflow
NEXT_PUBLIC_N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/language-change

# Optional: API key for webhook authentication
NEXT_PUBLIC_N8N_API_KEY=your-webhook-api-key

# Internal webhook URL for server-to-server communication
N8N_INTERNAL_WEBHOOK_URL=https://your-n8n-instance.com/webhook/internal/language

# Secret key for validating n8n webhook calls to your API
N8N_WEBHOOK_SECRET=your-secret-key

# Your backend API URL (if applicable)
NEXT_PUBLIC_API_URL=https://api.civilarisglobal.com

# n8n API key for internal use
N8N_API_KEY=your-n8n-api-key